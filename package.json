{
	"name": "project-management_api",
	"version": "0.0.1",
	"description": "project management app API",
	"engines": {
		"node": "^20",
		"npm": "^10.5.0"
	},
	"main": "./node_modules/.bin/nodemon server.js",
	"scripts": {
		"start": "node --experimental-vm-modules server.js",
		"migrations": "node migration.js up",
		"seeds": "node seed.js up",
		"test": "node --expose-gc --no-compilation-cache --experimental-vm-modules ./node_modules/.bin/jest --logHeapUsage --silent --forceExit",
		"test:ci": "node --expose-gc --no-compilation-cache --experimental-vm-modules ./node_modules/.bin/jest --logHeapUsage --coverage --forceExit",
		"test:coverage": "node --expose-gc --no-compilation-cache --experimental-vm-modules ./node_modules/.bin/jest --silent --logHeapUsage --coverage",
		"eslint": "./node_modules/.bin/eslint api/** utility/ --quiet --fix",
		"dev": "npm run migrations && ./node_modules/.bin/nodemon --experimental-vm-modules server.js",
		"prod": "./node_modules/.bin/nodemon start --experimental-vm-modules server.js",
		"server": "./node_modules/.bin/nodemon --experimental-vm-modules server.js",
		"generate:swagger": "node ./utility/swagger-gen.js",
		"migrate:v1": "node ./utility/migration-scripts/v1ResponseData.js",
		"migrate:localProductId": "node ./utility/migration-scripts/insertLocalProductIds.js",
		"migrate:questionUuidHash": "node ./utility/migration-scripts/questionUuidMigrationHash.js",
		"migrate:sectionUuidHash": "node ./utility/migration-scripts/sectionUuidMigrationHash.js",
		"migrate:backfillShortLinks": "node ./utility/migration-scripts/backfillShortLinks.js",
		"migrate:taxonomyAndTerms": "node ./utility/migration-scripts/taxonomyAndTermsNewTags.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/afarghaly10/projectManagement-api"
	},
	"keywords": [
		"api",
		"restful",
		"node",
		"mysql"
	],
	"author": "Fekret Cloud",
	"license": "MIT",
	"homepage": "",
	"dependencies": {
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/morgan": "^1.9.9",
		"@types/node": "^22.4.0",
		"akismet-api": "^5.3.0",
		"apicache-plus": "^2.3.1",
		"axios": "^1.6.8",
		"axios-retry": "4.1.0",
		"bluebird": "^3.5.4",
		"compression": "^1.7.4",
		"cors": "^2.8.4",
		"crypto": "^1.0.1",
		"crypto-js": "^4.1.1",
		"dd-trace": "^5.24.0",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"express-xml-bodyparser": "^0.3.0",
		"fast-json-stable-stringify": "^2.1.0",
		"helmet": "^7.1.0",
		"ioredis": "^5.4.1",
		"joi": "^17.7.0",
		"jsonwebtoken": "^9.0.0",
		"jstat": "^1.9.6",
		"jwt-decode": "4.0.0",
		"lodash": "^4.17.21",
		"mysql-migrations": "^1.0.7",
		"mysql2": "3.10.2",
		"uuid": "^9.0.0",
		"http-status": "^1.5.3",
		"passport": "^0.6.0",
		"passport-azure-ad-oauth2": "0.0.4",
		"passport-saml": "^3.2.4"
	},
	"overrides": {
		"xml2js": "^0.5.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.2.0",
		"@faker-js/faker": "8.4.1",
		"@types/jest": "^29.5.12",
		"async-csv": "^2.1.3",
		"eslint": "^9.2.0",
		"eslint-config-google": "^0.14.0",
		"eslint-plugin-jest": "^28.5.0",
		"globals": "^15.1.0",
		"jest": "^29.7.0",
		"jest-cli": "^29.7.0",
		"nodemon": "^3.1.0",
		"sinon": "17.0.1",
		"supertest": "7.0.0",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "5.0.0"
	}
}
